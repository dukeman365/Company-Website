<div class="page">
  <div id="about__card" class="shadow card">

    <div class="section__content">

        <!--Start Section-->
        {{#each sections}}
          <div class="col-xs-12 col-md-4">
            <div class="shadow card margin--bottom">
              <div class="shadow section__header">
                <div class="section__title">
                  {{title}}
                </div>
              </div>
              <!--Start section content-->
              <div class="section__content">
                {{content}}
              </div>
              <!--End Section content-->
            </div>
          </div>
        {{/each}}
        <!--End Section-->
      </div>


  </div>
</div>
